{
  "version": 2,
  "builds": [
    {
      "src": "packages/server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "packages/client/dist",
      "use": "@vercel/static"
    }
  ],

  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "packages/server/server.js"
    }
  ]
}
